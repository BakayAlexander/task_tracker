{
  "quoteProps": "as-needed",
  "singleQuote": true,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "useTabs": false,
  "proseWrap": "always",
  "bracketSpacing": true,
  "trailingComma": "es5",
  "semi": true,
  "jsxSingleQuote": true,
  "jsxBracketSameLine": false,
  "bracketSameLine": false,
  "singleAttributePerLine": true
}
